{
    "name": "darkbiz",
    "description": "darkbiz is a responsive professional premium blogging template",
    "demo": "https://darkbiz.themeix.com/",
    "version": "1.0.1",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v2"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan ."
    },
    "author": {
        "name": "Themeix",
        "email": "themeix@gmail.com",
        "url": "https://themeix.com/"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "devDependencies": {
        "gulp": "^4.0.0",
        "gulp-watch": "^5.0.1"
    },
    "config": {
        "posts_per_page": 14,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {
        "gulp-sass": "^4.0.2",
        "gulp-scss": "^1.4.0",
        "gulp-sourcemaps": "^2.6.5"
    }
}
